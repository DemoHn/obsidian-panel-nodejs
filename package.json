{
  "name": "obsidian-panel",
  "version": "0.6.1",
  "description": "A Minecraft Server Management Panel (commercial)",
  "main": "start.js",
  "scripts": {
    "test": "mocha --recursive",
    "init": "npm install && cd frontend && npm install && cd .. ",
    "build": "node bin/build.js",
    "clean": "rm -rf dist",
    "panel": "node start-panel.js",
    "count": "cloc --exclude-dir=node_modules,frontend,env,static ."
  },
  "author": "Nigshoxiz",
  "license": "UNLICENSED",
  "dependencies": {
    "blueimp-md5": "^2.7.0",
    "body-parser": "^1.17.1",
    "colors": "^1.1.2",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "fs-extra": "^2.1.2",
    "ftpd": "^0.2.15",
    "getos": "^2.8.4",
    "iconv-lite": "^0.4.17",
    "is-plain-object": "^2.0.1",
    "js-yaml": "^3.8.2",
    "jschardet": "^1.4.2",
    "mkdirp": "^0.5.1",
    "morgan": "^1.8.1",
    "multer": "^1.3.0",
    "mysql": "^2.13.0",
    "node-service-linux": "github:demohn/node-service-linux",
    "pidusage": "^1.1.3",
    "proc-cpuinfo": "^1.0.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.3",
    "sequelize": "^3.30.3",
    "socket.io": "^1.7.3",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "supertest": "^3.0.0"
  },
  "pkg": {
    "scripts": [
      "./app/**/*.js",
      "./tools/**/*.js",
      "./ftp_manager/**/*.js",
      "./node_modules/shelljs/src/*.js",
      "start-panel.js"
    ],
    "assets": [
      "./static/**/*",
      "config.yml.sample",
      "package.json",
      "./tools/**/*.js"
    ],
    "dirs": [
      "./app/model",
      "./tools"
    ]
  }
